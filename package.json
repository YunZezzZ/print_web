{
  "name": "print_web",
  "version": "1.0.0",
  "description": "print_web",
  "main": "index.js",
  "build": {
    "productName": "print_web",
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true
    },
    "win": {
      "icon": "img/256.png",
      "target": [
        "nsis",
        "zip"
      ],
      "asarUnpack": [
        "./jar${/*}",
        "./jre${/*}"
      ]
    }
  },
  "scripts": {
    "start": "electron .",
    "package": "electron-builder --win --x64"
  },
  "author": "YunZezzZ",
  "devDependencies": {
    "electron": "^15.5.5",
    "electron-builder": "^21.1.5",
    "electron-packager": "^14.0.3",
    "electron-squirrel-startup": "^1.0.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "minimal-request-promise": "^1.5.0",
    "request": "^2.88.0",
    "tree-kill": "^1.2.1"
  }
}
